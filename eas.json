{
  "cli": {
    "version": ">=7.6.0",
    "appVersionSource": "local"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_APP_VARIANT": "development"
      }
    },
    "preview": {
      "distribution": "internal",
      "autoIncrement": false,
      "credentialsSource": "remote",
      "env": {
        "EXPO_PUBLIC_APP_VARIANT": "internalTesting"
      },
      "android": {
        "buildType": "app-bundle"
      }
    },
    "apk": {
      "distribution": "internal",
      "autoIncrement": false,
      "credentialsSource": "remote",
      "env": {
        "EXPO_PUBLIC_APP_VARIANT": "apkTesting"
      },
      "android": {
        "buildType": "apk"
      }
    },
    "production": {
      "distribution": "store",
      "autoIncrement": false,
      "credentialsSource": "remote",
      "env": {
        "EXPO_PUBLIC_APP_VARIANT": "production"
      },
      "android": {
        "buildType": "app-bundle"
      }
    }
  },
  "submit": {
    "production": {
      "android": {
        "track": "internal",
        "releaseStatus": "draft"
      }
    }
  }
}
